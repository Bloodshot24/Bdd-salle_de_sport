"SELECT ID_Membre, Nom, Prenom, Mail, Date_Inscription FROM Membre WHERE Validite_Inscription = FALSEORDER BY Date_Inscription ASC"
"SELECT Validite_Inscription FROM Membre WHERE ID_Membre = @id"
"UPDATE Membre SET Validite_Inscription = TRUE WHERE ID_Membre = @id"
"SELECT ID_Membre, Nom, Prenom, Mail FROM Membre WHERE Validite_Inscription = TRUE ORDER BY Nom ASC, Prenom ASC"
"SELECT ID_Membre, Nom, Prenom, Mail FROM Membre WHERE Validite_Inscription = FALSE ORDER BY Date_Inscription ASC"
"DELETE FROM Membre WHERE ID_Membre = @id"
"SELECT ID_Membre, Nom, Prenom, Mail, Date_Inscription FROM Membre WHERE Validite_Inscription = TRUE ORDER BY Nom ASC, Prenom ASC"
 "DELETE FROM Inscription WHERE ID_Membre = @id"
 "SELECT ID_Membre, Nom, Prenom, Adresse, Tel, Mail, Date_Inscription, Validite_Inscription FROM Membre WHERE ID_Membre = @id"
"SELECT COUNT(*) FROM Membre WHERE Validite_Inscription = FALSE"
"UPDATE Membre SET Nom = @nom, Prenom = @prenom, Adresse = @adresse, Tel = @tel, Mail = @mail WHERE ID_Membre = @id"
"INSERT INTO Coach (Nom, Prenom, Tel, MDP, Formation, Specialite) VALUES (@nom, @prenom, @tel, @mdp, @formation, @specialite)"
"SELECT ID_Coach, Nom, Prenom, Tel, Formation, Specialite FROM Coach ORDER BY Nom ASC, Prenom ASC"
"SELECT ID_Cours, Nom_Cours, Description, Duree_h_min, Intensite, Difficulte, Capacite FROM Cours ORDER BY Nom_Cours ASC"
"SELECT c.Nom_Cours, COUNT(i.ID_Membre) as NbInscrits FROM Cours c LEFT JOIN Inscription i ON c.ID_Cours = i.ID_Cours GROUP BY c.ID_Cours, c.Nom_Cours ORDER BY NbInscrits DESC LIMIT 1"
"SELECT c.ID_Cours, c.Nom_Cours, c.Description, c.Duree_h_min, c.Intensite, c.Difficulte, c.Capacite, COUNT(i.ID_Membre) as NbInscrits FROM Cours c LEFT JOIN Inscription i ON c.ID_Cours = i.ID_Cours GROUP BY c.ID_Cours ORDER BY c.Nom_Cours ASC"
"INSERT INTO Membre (Nom, Prenom, Adresse, Tel, Mail, Date_Inscription, Validite_Inscription, MDP, ID_Admin)VALUES (@nom, @prenom, @adresse, @tel, @mail, NOW(),FALSE, @mdp, NULL)"